{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Api": {
    "BaseUrl": "https://localhost:7001",
    "AuthServer": "https://localhost:7001",
    "ClientId": "sdms_frontend"
  },
  "Frontend": {
    "Url": "http://localhost:4200",
    "AuthCallbackUrl": "http://localhost:4200/auth-callback"
  },
  "Deployment": {
    "Platform": "Vercel",
    "BuildCommand": "cd ClientApp && npm install && npm run build",
    "OutputDirectory": "ClientApp/dist/sdms-b2c-webapp"
  }
}

